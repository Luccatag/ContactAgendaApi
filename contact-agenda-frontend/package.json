{
  "name": "contact-agenda-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:fixed": "vitest run src/test/basic.test.ts src/test/services/contactService.test.ts src/test/stores/contactStore.fixed.test.ts src/test/components/components.fixed.test.ts src/test/components/ContactCard.fixed.test.ts src/test/components/ContactForm.fixed.test.ts",
    "test:all-fixed": "npm run test:fixed"
  },
  "dependencies": {
    "@primeuix/themes": "^1.2.1",
    "axios": "^1.10.0",
    "pinia": "^3.0.3",
    "primevue": "^4.3.6",
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.0.15",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  }
}
